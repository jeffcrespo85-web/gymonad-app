{
  "name": "gymonad-web3-fitness-app",
  "version": "1.0.0",
  "private": true,
  "description": "Web3 fitness app that rewards users with $GYM tokens for working out on Monad Testnet",
  "keywords": [
    "web3",
    "fitness",
    "crypto",
    "monad",
    "blockchain",
    "gym",
    "tokens"
  ],
  "license": "MIT",
  "author": "Gymonad Team",
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "clean": "rm -rf .next out dist",
    "coverage:contracts": "npx hardhat coverage",
    "db:generate": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > types/supabase.ts",
    "db:migrate": "node scripts/run-sql-scripts.js",
    "deploy:contracts": "npx hardhat run scripts/deploy.js --network monad-testnet",
    "deploy:preview": "vercel",
    "deploy:vercel": "vercel --prod",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "export": "next build && next export",
    "install:clean": "rm -rf node_modules package-lock.json && npm install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "test:contracts": "npx hardhat test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
